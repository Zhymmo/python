import pygame
class fly_sprit(pygame.sprite.Sprite):
    
    def __init__(self,image_name,speed=1,health=5):
        super().__init__()
        self.image=pygame.image.load(image_name)
        self.rect=self.image.get_rect()#函数返回图像大小
        self.speed=speed
        self.health=health

    def update(self):
        self.rect.y += self.speed
